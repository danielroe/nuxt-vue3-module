    "vue-loader": "16.8.1",
    "vue-server-renderer": "npm:@vue/server-renderer@3.2.20",
    "vue-template-compiler": "npm:@vue/compiler-sfc@3.2.20",
    "vuex": "3.6.2"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "7.15.8",

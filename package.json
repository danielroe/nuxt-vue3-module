{
  "name": "nuxt-vue3",
  "version": "0.1.2",
  "description": "Timings integration for Nuxt",
  "keywords": [
    "nuxt",
    "module",
    "nuxt-module",
    "timings",
    "performance"
  ],
  "repository": "danielroe/nuxt-vue3-module",
  "license": "MIT",
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./dist/package.json",
    "./templates/*": "./dist/templates/*"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "siroc build",
    "dev": "nuxt example",
    "lint": "eslint --ext .js,.ts,.vue",
    "lint:all": "yarn lint .",
    "prepare": "husky install && yarn build",
    "prepublishOnly": "yarn test",
    "release": "yarn build && yarn test && release-it",
    "test": "yarn lint && yarn build && yarn jest"
  },
  "dependencies": {
    "@nuxt/kit": "0.6.2",
    "@vue/compat": "3.1.1",
    "@vue/compiler-sfc": "3.1.1",
    "@vue/server-renderer": "3.1.1",
    "chalk": "4.1.1",
    "fs-extra": "9.1.0",
    "upath": "2.0.1",
    "vue": "3.1.1",
    "vue-loader": "16.2.0",
    "vue-server-renderer": "npm:@vue/server-renderer@3.1.1",
    "vue-template-compiler": "npm:@vue/compiler-sfc@3.1.1",
    "vuex": "3.6.2"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "7.14.5",
    "@babel/preset-env": "7.14.5",
    "@babel/preset-typescript": "7.14.5",
    "@nuxt/test-utils": "0.2.2",
    "@nuxt/types": "2.15.6",
    "@nuxt/typescript-build": "2.1.0",
    "@nuxtjs/eslint-config-typescript": "6.0.1",
    "@release-it/conventional-changelog": "2.0.1",
    "@types/fs-extra": "9.0.11",
    "@types/jest": "26.0.23",
    "babel-eslint": "latest",
    "babel-jest": "27.0.2",
    "eslint": "7.28.0",
    "eslint-config-prettier": "8.3.0",
    "husky": "6.0.0",
    "jest": "27.0.4",
    "lint-staged": "11.0.0",
    "nuxt-edge": "2.16.0-27050421.cc3f43e1",
    "nuxt-vite": "0.1.1",
    "release-it": "14.7.0",
    "siroc": "0.11.0",
    "vite-plugin-vue2": "1.6.2"
  },
  "peerDependencies": {
    "chalk": "^3.0.0 || ^4.0.0",
    "consola": "^2.15.0",
    "prettier": "^2.1.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "volta": {
    "node": "14.17.1"
  }
